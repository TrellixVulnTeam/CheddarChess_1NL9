{
    "name": "Django Channels",
    "description": "A real-time ajedrez",
    "repository": "https://",
    "keywords": ["django", "channels", "websockets"],
    "scripts": {
        "postdeploy": "python manage.py migrate"
    },
    "env": {
        "SECRET_KEY": {
            "description": "Django's SECRET_KEY setting",
            "generator": "secret"
        }
    },
    "addons": ["heroku-postgresql"],
    "formation": {
        "web": {
            "quantity": 1,
          " size": "Free"
        },
        "worker": {
            "quantity": 1,
            "size": "Free"
        }
    }
	"jugador": {user.username}, 
	"fen": {fen}, 
	"partida":{partida},
	"fecha": {fecha},
	"hora": {hora},
	"contrincante": {contrincante}
}